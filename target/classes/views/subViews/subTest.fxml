<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.subViewController.Test"
            prefHeight="400.0" prefWidth="600.0">

    <VBox alignment="CENTER" layoutX="250.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <children>
            <Label text="Hello, FXML!" />
            <Label fx:id="theButtonLabel" text="This is a label." />
            <Button fx:id="theButton" onAction="#handleButtonClick" text="Click Me" />
        </children>
    </VBox>

</AnchorPane>
